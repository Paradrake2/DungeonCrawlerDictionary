{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run UI (WPF) - MyWpfApp",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-ui",
      "program": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/my-wpf-app/my-wpf-app.csproj"
      ],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
    }
  ]
}
